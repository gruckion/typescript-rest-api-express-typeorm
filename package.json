{
   "name": "typescript-rest-api-express",
   "version": "0.0.1",
   "author": "Gruckion",
   "description": "Typescript REST API using express and typeORM",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/gruckion/typescript-rest-api-express.git"
   },
   "bugs": {
      "url": "https://github.com/gruckion/typescript-rest-api-express/issues"
   },
   "homepage": "https://github.com/gruckion/typescript-rest-api-express#readme",
   "license": "MIT",
   "scripts": {
      "build": "tsc",
      "start": "set debug=* && ts-node-dev --respawn --transpileOnly ./src/index.ts",
      "prod": "tsc && node ./build/app.js",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "lint": "eslint --fix \"src/**/*\""
   },
   "devDependencies": {
      "@types/node": "^8.0.29",
      "ts-node": "3.3.0",
      "typescript": "^3.3.3333"
   },
   "dependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.0",
      "@types/cors": "^2.8.5",
      "@types/helmet": "0.0.43",
      "@types/jsonwebtoken": "^8.3.2",
      "@typescript-eslint/eslint-plugin": "^1.9.0",
      "@typescript-eslint/parser": "^1.9.0",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-validator": "^0.9.1",
      "cors": "^2.8.5",
      "eslint": "^5.16.0",
      "eslint-config-prettier": "^4.3.0",
      "eslint-config-typescript": "^2.0.0",
      "eslint-plugin-filenames": "^1.3.2",
      "eslint-plugin-jest": "^22.6.4",
      "eslint-plugin-prettier": "^3.1.0",
      "eslint-plugin-react": "^7.13.0",
      "express": "^4.15.4",
      "helmet": "^3.18.0",
      "jsonwebtoken": "^8.5.1",
      "prettier": "^1.17.1",
      "reflect-metadata": "^0.1.10",
      "sqlite3": "^4.0.3",
      "ts-node-dev": "^1.0.0-pre.39",
      "typeorm": "^0.2.25"
   }
}